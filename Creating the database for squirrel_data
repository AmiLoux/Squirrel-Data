CREATE TABLE squirrel_data (
	x FLOAT(30),
	y FLOAT(30),
	unique_squirrel_id VARCHAR(50),
	hectare VARCHAR(3),
	shift VARCHAR(2),
	date DATE,
	hectare_squirrel_number INTEGER,
	age INTEGER,
	primary_fur_colour VARCHAR(50),
	highlight_fur_colour VARCHAR(50),
	combination_of_primary_and_highlight_colour VARCHAR(50),
	colour_notes VARCHAR(100),
	location VARCHAR(10000),
	above_ground_sighter_measurement VARCHAR(5),
	specific_location VARCHAR(10000),
	running BOOLEAN,
	chasing BOOLEAN,
	climbing BOOLEAN,
	eating BOOLEAN,
	foraging BOOLEAN,
	other_activites VARCHAR(250),
	kuks BOOLEAN,
	quass BOOLEAN,
	moans BOOLEAN,
	tail_flags BOOLEAN,
	tail_twitches BOOLEAN,
	approaches BOOLEAN,
	indifferent BOOLEAN,
	runs_from BOOLEAN,
	other_interactions VARCHAR(250),
	lat_long VARCHAR(100)
	);
